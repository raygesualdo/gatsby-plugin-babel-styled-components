{
  "name": "@raygesualdo/gatsby-plugin-babel-styled-components",
  "version": "0.0.0-development",
  "description": "Gatsby plugin to add the styled-components babel plugin",
  "scripts": {
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raygesualdo/gatsby-plugin-babel-styled-components.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "styled-components",
    "babel"
  ],
  "author": "Ray Gesualdo <ray@raygesualdo.com> (https://raygesualdo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raygesualdo/gatsby-plugin-babel-styled-components/issues"
  },
  "homepage": "https://github.com/raygesualdo/gatsby-plugin-babel-styled-components#readme",
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "semantic-release": "^15.1.2"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
